  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac
  make -f /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac
  make -f /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac
  make -f /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/CMakeScripts/ReRunCMake.make
