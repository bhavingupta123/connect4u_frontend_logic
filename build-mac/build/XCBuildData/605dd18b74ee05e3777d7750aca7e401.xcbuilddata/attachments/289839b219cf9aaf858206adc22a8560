#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/engine/extensions/ImGui
  /Users/bhavin.gupta/Documents/axmol_setup/axmol/tools/external/axslcc/axslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/bhavin.gupta/Documents/axmol_setup/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/bhavin.gupta/Documents/axmol_setup/axmol/extensions/ImGui/shaders/imgui_sprite.vert --output=/Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/runtime/axslc/custom/imgui_sprite_vs
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/engine/extensions/ImGui
  /Users/bhavin.gupta/Documents/axmol_setup/axmol/tools/external/axslcc/axslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/bhavin.gupta/Documents/axmol_setup/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/bhavin.gupta/Documents/axmol_setup/axmol/extensions/ImGui/shaders/imgui_sprite.vert --output=/Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/runtime/axslc/custom/imgui_sprite_vs
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/engine/extensions/ImGui
  /Users/bhavin.gupta/Documents/axmol_setup/axmol/tools/external/axslcc/axslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/bhavin.gupta/Documents/axmol_setup/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/bhavin.gupta/Documents/axmol_setup/axmol/extensions/ImGui/shaders/imgui_sprite.vert --output=/Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/runtime/axslc/custom/imgui_sprite_vs
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/engine/extensions/ImGui
  /Users/bhavin.gupta/Documents/axmol_setup/axmol/tools/external/axslcc/axslcc --silent --err-format=msvc --lang=msl --automap --no-suffix "--defines=METAL" --include-dirs=/Users/bhavin.gupta/Documents/axmol_setup/axmol/core/renderer/shaders --sgs --reflect --vert=/Users/bhavin.gupta/Documents/axmol_setup/axmol/extensions/ImGui/shaders/imgui_sprite.vert --output=/Users/bhavin.gupta/Documents/axmol_project/project2/Connect4Game/build-mac/runtime/axslc/custom/imgui_sprite_vs
fi

